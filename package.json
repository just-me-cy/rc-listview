{
  "name": "rc-listview",
  "version": "1.0.3",
  "description": "vertical listview based on react",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js,.jsx src examples",
    "compile": "rimraf lib && babel src --copy-files --out-dir lib --source-maps --ignore src/examples",
    "start": "webpack-dev-server --devtool eval --progress --colors --content-base build",
    "test": "mocha --compilers js:babel-register --require ./test/setup.js --require ignore-styles",
    "deploy":"NODE_ENV=production webpack -p --config webpack.product.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/just-me-cy/rc-listview.git"
  },
  "keywords": [
    "react",
    "listview"
  ],
  "author": "just-me-cy <chenyao_0511@126.com> (https://github.com/just-me-cy)",
  "license": "MIT",
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-motion": "^0.4.4"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "html-webpack-plugin": "^2.22.0",
    "ignore-styles": "^4.0.0",
    "jsdom": "^9.4.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.0.0",
    "mochawesome": "^1.4.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.3.0",
    "react-hot-loader": "^1.3.0",
    "react-motion": "^0.4.4",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
